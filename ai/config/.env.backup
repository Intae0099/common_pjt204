###############################################
#  🔖  .env.template — 데이터베이스 설정 템플릿
#  이 파일을 복사(.env)한 뒤 실제 값으로 채워서 사용하세요.
###############################################

# DB 접속 계정 이름 (예: admin, app_user 등)
DB_USER=

# DB 계정 비밀번호 — 절대 깃 저장소에 커밋하지 마세요!
DB_PASSWORD=

# DB 서버 호스트
#   - 로컬 개발용: localhost 또는 127.0.0.1
#   - 원격 서버: db.example.com, 10.0.0.5 등
DB_HOST=

# DB 포트
#   - PostgreSQL 기본값은 5432
DB_PORT=

# 접속할 데이터베이스 이름
#   - 초기 생성할 때 지정한 스키마명
DB_NAME=